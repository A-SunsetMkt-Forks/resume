{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter {packages/*} dev",
    "build": "pnpm --filter {packages/*} build",
    "play": "pnpm --filter ./playground dev",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp packages/*/package.json --no-verify --commit --push --tag && pnpm --filter {packages/*} publish --access public",
    "check-type": "tsc --noEmit"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@types/node": "^17.0.33",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "pnpm": "^7.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
