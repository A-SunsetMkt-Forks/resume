{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "dev": "pnpm --parallel --filter {packages/*} --filter {templates/*} dev",
    "build": "pnpm --filter {packages/*} --filter {templates/*} build",
    "play": "pnpm --filter ./playground dev",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp packages/*/package.json templates/*/package.json --no-verify --commit --push --tag",
    "check-type": "tsc --noEmit"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-react": "^0.35.2",
    "@resumejs/components": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jsdom": "^21.1.0",
    "pnpm": "^7.27.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0"
    }
  }
}
