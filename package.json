{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "dev": "pnpm --parallel --filter {packages/*} --filter {templates/*} dev",
    "build": "pnpm --parallel --filter {packages/*} --filter {templates/*} build",
    "play": "pnpm --filter ./playground dev",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp packages/*/package.json templates/*/package.json --no-verify --commit --push --tag",
    "check-type": "tsc --noEmit"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@resumejs/components": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "jsdom": "^20.0.0",
    "pnpm": "^7.12.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0"
    }
  }
}
